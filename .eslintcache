nts.length === 0) {\n            transformedIngredients = <p>Please, start adding ingredients</p>\n        }\n    return (\n        <div className={styles.Burger}>\n            <BurgerIngredient type=\"bread-top\" />\n                {transformedIngredients}\n            <BurgerIngredient type=\"bread-bottom\" />\n        </div>\n    );\n}\n\nexport default burger;","/Users/yara/Projetcs/burger-app/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/yara/Projetcs/burger-app/src/components/Burger/BuildControls/BuildControls.js",[],"/Users/yara/Projetcs/burger-app/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/Users/yara/Projetcs/burger-app/src/components/UI/Modal/Modal.js",[],"/Users/yara/Projetcs/burger-app/src/components/OrderSummary/OrderSummary.js",[],"/Users/yara/Projetcs/burger-app/src/components/UI/Backdrop/Backdrop.js",[],"/Users/yara/Projetcs/burger-app/src/components/UI/Button/Button.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/Logo/Logo.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/NavigationItems/NavigationItems.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/NavigationItems/NavigationItem/NavigationItem.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/SideDrawer/SideDrawer.js",[],"/Users/yara/Projetcs/burger-app/src/components/Navigation/SideDrawer/menuButton/MenuButton.js",[],"/Users/yara/Projetcs/burger-app/src/containers/Layout/Layout.js",[],"/Users/yara/Projetcs/burger-app/src/hoc/Aux/Aux.js",["192"],"import React from 'react';\n\nconst aux = (props) => props.children;\n\nexport default aux;","/Users/yara/Projetcs/burger-app/src/axios-orders.js",[],"/Users/yara/Projetcs/burger-app/src/components/UI/Spinner/Spinner.js",[],"/Users/yara/Projetcs/burger-app/src/hoc/withErrorHandler/withErrorHandler.js",[],"/Users/yara/Projetcs/burger-app/src/containers/Chec